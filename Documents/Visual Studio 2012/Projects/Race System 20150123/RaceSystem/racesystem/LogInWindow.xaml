<Window x:Class="RaceSystem.LogInWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Log In" Height="139.5" Width="339">
    <Grid Margin="0,0,0,-4">
        <Label Content="Username" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="74"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="10,36,0,0" VerticalAlignment="Top" Width="74"/>
        <TextBox x:Name="tfUserName" Height="23" Margin="84,10,10,0" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <PasswordBox x:Name="tfPassword" Margin="84,38,10,0" VerticalAlignment="Top"/>
        <Button x:Name="btnLogIn" Content="Log In" HorizontalAlignment="Left" Margin="70,67,0,0" VerticalAlignment="Top" Width="76" Click="btnLogIn_Click"/>
        <Button x:Name="btnCancel" Content="Cancel" HorizontalAlignment="Left" Margin="182,67,0,0" VerticalAlignment="Top" Width="76" Click="btnCancel_Click"/>
        
    </Grid>
</Window>
